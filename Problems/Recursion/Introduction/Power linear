import java.util.*;

public class Main{
  public static void main(String[] args)[
      Scanner in = new Scanner(System.in);
      int n = in.nextInt();
      int x = in.nextInt();
      int ans= powerLinear(x,n);
      System.out.println(ans);
  }
  
  public static int powerLinear(int x, int n){
      //Base Case 
      if(n==0) return 1;
      
      return n*powerLinear(x,n-1);
   }
   
}
