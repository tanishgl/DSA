import java.util.*;

public class Main{
  public static void main(String[] args)[
      Scanner in = new Scanner(System.in);
      int n = in.nextInt();
      zigZag(n);
  }
  
  public static void zigZag(int n){
    //Base Case
    if(n==0) return;
    
    System.out.println(n);
    zigZag(n-1);
    System.out.println(n);
    zigZag(n-1);
    System.out.println(n);
    
  }
  
}
