import java.util.*;

public class Main{
  public static void main(String[] args)[
      Scanner in = new Scanner(System.in);
      int n = in.nextInt();
      decinc(n);
  }
  
  public static void decinc(int n){
      //Base Case
      if(n==0) return;
      
      //Work
      System.out.println(n);
      
      //Faith
      decinc(n-1);
      
      //Work
      System.out.println(n);
 }
 
}
