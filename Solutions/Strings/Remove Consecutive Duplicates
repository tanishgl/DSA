import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String s = in.nextLine();
        String str = "";
        for(int i=0;i<s.length();i++){
            if(i==0 || s.charAt(i)!=s.charAt(i-1)){
                str += s.charAt(i);
            }
        }
        System.out.println(str);
    }
}
