import java.util.*;
   
   public class Main{
   
   public static void main(String[] args) {
     Scanner in = new Scanner(System.in);
     int n = in.nextInt();
     int x = n;
     int digits = 0;
     while(n/10>0){
       digits++;
       n/=10;
     }

     for(int i=0;i<=digits;i++){
       System.out.println((x/(int)Math.pow(10,i))%10);
     }
    }
   }
